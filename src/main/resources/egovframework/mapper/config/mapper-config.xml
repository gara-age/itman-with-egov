<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!--Mybatis 설정 -->
	<settings>
		<!-- 전통적인 데이터베이스 컬럼명 형태인 A_COLUMN을 CamelCase형태의 자바 프로퍼티명 형태인 aColumn으로 자동으로 매핑하도록 함 -->
		<setting name="mapUnderscoreToCamelCase" value="true"></setting>
		<!--  파라미터에 Null 값이 있을 경우 에러 처리 -->
		<setting name="jdbcTypeForNull" value="VARCHAR"></setting>
	</settings>
	
	<!-- Type Aliases 설정-->
	<typeAliases>
		<typeAlias alias="egovMap" 			type="org.egovframe.rte.psl.dataaccess.util.EgovMap" />
		<typeAlias alias="FileVO"			type="egovframework.com.cmm.service.FileVO" />
		<typeAlias alias="ComDefaultCodeVO" type="egovframework.com.cmm.ComDefaultCodeVO" />
		<typeAlias alias="comDefaultVO"		type="egovframework.com.cmm.ComDefaultVO" />
		<typeAlias alias="employeeVO"		type="egovframework.itman.employee.service.EmployeeVO" />
		<typeAlias alias="divisionVO"		type="egovframework.itman.division.service.DivisionVO" />
		<typeAlias alias="empStateVO"  		type="egovframework.itman.empState.service.EmpStateVO" />
		<typeAlias alias="positionVO"		type="egovframework.itman.position.service.PositionVO" />
		<typeAlias alias="pagination" 		type="egovframework.itman.common.Pagination" />
		<typeAlias alias="assetVO"			type="egovframework.itman.asset.service.AssetVO" />
		<typeAlias alias="assetCategoryVO" 	type="egovframework.itman.assetCategory.service.AssetCategoryVO"/>
		<typeAlias alias="stateVO" 			type="egovframework.itman.state.service.StateVO"/>
		<typeAlias alias="supplierVO" 		type="egovframework.itman.supplier.service.SupplierVO"/>
		<typeAlias alias="locationVO" 		type="egovframework.itman.location.service.LocationVO" />
		<typeAlias alias="softwareVO" 		type="egovframework.itman.software.service.SoftwareVO" />
		<typeAlias alias="hardwareVO" 		type="egovframework.itman.hardware.service.HardwareVO" />
		<typeAlias alias="assLogVO" 		type="egovframework.itman.assLog.service.AssLogVO" />
		<typeAlias alias="groupVO" 			type="egovframework.itman.group.service.GroupVO" />
	</typeAliases>
    
</configuration>